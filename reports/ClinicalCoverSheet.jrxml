					<paragraph lineSpacing="Single"/>
				</textElement>
				<text><![CDATA[WAIVER for PHIC Patients]]></text>
			</staticText>
			<staticText>
				<reportElement mode="Transparent" x="0" y="424" width="572" height="62" forecolor="#000000" backcolor="#FFFFFF" />
